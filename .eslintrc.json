{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "import/resolver": {
            "node": {
              "extensions": [".js", ".ts"]
            }
        }
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": ["error","never"],
        "import/prefer-default-export": "off",
        "no-unused-vars": "warn",
        "import/no-unresolved": "off"
    }
}
